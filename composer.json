{
    "name": "fmassei/qr-code",
    "description": "fmassei QR Code",
    "keywords": ["fmassei", "qrcode", "qr", "code", "php", "endroid"],
    "homepage": "https://github.com/fmassei/qr-code",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco Massei",
            "email": "fmassei@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "ext-fileinfo": "*",
        "ext-imagick": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "fmassei\\QrCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fmassei\\QrCode\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
